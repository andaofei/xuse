/* pages/home/index.wxss */
.cantiner{
  width: 100%;
  height: 100%;
}
.banner{
  width: 100%;
  height: 460rpx;
}
.banner_img{
  width: 100%;
  height: 100%;
}
.item-box{
  display: flex;
  flex-wrap: wrap;
}
.item_middle{
  display: flex;
  justify-content: space-between;
}
.item_img{
  width: 80rpx;
  height: 80rpx;
}
.item{
  display: flex;
  flex-direction: column;
  width: 33%;
  text-align: center;
  padding: 20rpx 0;
}
.item_text{
  font-size: 28rpx;
  line-height: 48rpx;
  color: #575C65;
}